                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT JobID, OfficeName AS Office, DepartmentName AS Department, JobNumber, JobName, Description AS Foreman
FROM tblJob j
LEFT JOIN tblOffice o ON j.OfficeID = o.OfficeID
LEFT JOIN tblDepartment d ON j.DepartmentID = d.DepartmentID
LEFT JOIN tblSuperintendent f ON j.SuperintendentID = f.SuperintendentID

WHERE 
	j.JobNumber &gt; '' AND  
	[dbo].[GetUserJobAccess] (JobID, 'ATEFBAKI') = 1 AND		
    (SELECT COUNT(h.JobNumber) FROM tblJobHour h WHERE h.JobNumber = j.JobNumber AND weekend =  '08/01/2010') &gt; 0  
ORDER BY Foreman

</CommandText>
